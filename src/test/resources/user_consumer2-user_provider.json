{
    "provider": {
        "name": "user_provider"
    },
    "consumer": {
        "name": "user_consumer2"
    },
    "interactions": [
        {
            "description": "get me a user paramesh",
            "request": {
                "method": "GET",
                "path": "/pact/user"
            },
            "response": {
                "status": 200,
                "body": {
                    "name": "paramesh",
                    "department": "IT"
                }
            },
            "providerStates": [
                {
                    "name": "user named subbu is available"
                }
            ]
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "3.0.0"
        },
        "pact-jvm": {
            "version": "3.5.11"
        }
    }
}